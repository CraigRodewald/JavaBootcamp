package com.gc.tdd;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

//Start a new class that validates everything as a String instead of the ArrayList

public class EvenNumberGeneratorStringTest {
	// Initialize constants
	EvenNumberGeneratorString engs = new EvenNumberGeneratorString();
	
//	 Runs first and instantiates engs so I don't have to repeat the constant
//		above in each test
	@Before
	public void setup() {
		engs = new EvenNumberGeneratorString();
	}
	
	@Test
	public void mustInstantiateGenerator() throws Exception {
		engs = new EvenNumberGeneratorString();
		assertNotNull(engs);
	}
}
