
public class EnhancedForLoop {
	public static void main(String[] args) {
		int[] numbers = {10, 20, 30, 40, 50};
		
		for (int i : numbers) {
			System.out.println(i);
			System.out.println(",");
		}
		
		System.out.println("");
		
		String[] names = {"James", "Larry", "Tom", "Lacy"};
		
		for (String name : names) {
			System.out.println(name);
			System.out.println(",");
		}
	}
}
