
public class Practice {
	public static void main(String[] args) {
		System.out.println("If i = 10:\n");
		int i = 10;
		System.out.println("i++ =" + i++);
		i = 10;
		System.out.println("++i =" + ++i);
		i = 10;
		System.out.println("--i =" + --i);
		i = 10;
		System.out.println("i-- =" + i--);
	
	}
}
